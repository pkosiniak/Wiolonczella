{
   "eslint.enable": true,
   "eslint.validate": [
      "javascript",
      "javascriptreact",
      {
         "language": "typescriptreact",
         "autoFix": true
      },
      {
         "language": "typescript",
         "autoFix": true
      }
   ],
   "multiCommand.commands": [{
      "command": "multiCommand.formatAndFix",
      "interval": 50,
      "sequence": [
         "editor.action.formatDocument",
         "eslint.executeAutofix",
         "editor.action.autoFix",
      ]
   }],
   "tslint.configFile": "./tslint.json",
   "cSpell.words": [
      "Checkmark",
      "Unmount",
      "monospace",
      "scrollbar"
   ],
}
